{
  "name": "component-library",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/shariqs/component-library"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public -o ./storybook",
    "build": "rm -rf dist && npm run build:prod",
    "build-dev": "cross-env NODE_ENV=development rollup -c",
    "build-prod": "cross-env NODE_ENV=production rollup -c",
    "prettier": "prettier --write \"src/**/*.+(js|jsx|json|css|md)\"",
    "linter": "eslint --fix \"src/**/*.+(js|jsx|css)\"",
    "lint": "npm run linter && npm run prettier"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3",
    "antd": "^4.17.4"
  },
