    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^24.9.1",
    "antd": "^4.17.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
